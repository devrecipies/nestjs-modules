  },
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false,
  "keywords": [
    "nestjs",
    "modules",
    "typescript",
    "backend"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/nestjs-modules"
  },
  "bugs": {
    "url": "https://github.com/your-username/nestjs-modules/issues"
  },
  "homepage": "https://github.com/your-username/nestjs-modules#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0"
  }
}
