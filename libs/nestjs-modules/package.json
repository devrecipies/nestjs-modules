{
  "name": "@devrecipies/nestjs-modules",
  "version": "0.0.2-8",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.js",
    "index.js.map",
    "index.d.ts",
    "index.d.ts.map",
    "lib/**/*.js",
    "lib/**/*.js.map",
    "lib/**/*.d.ts",
    "lib/**/*.d.ts.map",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "directory": "dist/libs/nestjs-modules",
    "access": "public"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "tslib": "^2.3.0"
  }
}
