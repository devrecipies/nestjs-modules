{
  "name": "@devrecipies/nestjs-modules",
  "version": "0.0.8",
  "description": "A collection of NestJS modules for common functionality",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
